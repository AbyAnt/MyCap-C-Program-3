#include<iostream>
using namespace std;

int main(){
int age,gender,M,F;
cout<<"Enter Age of a User:";
cin>>age;
if (age>=18){
cout<< "\nYou are eligible for voting";
cout<< "\nWhat is your gender:M or F";
cin>>gender;
if (gender = M){
cout<<" Gender= Male "<<M;
} else {
cout<<" Gender=Female"<<F;
}
} else {
cout<<"\nYou are not eligible for voting";
}

return 0;
}
